{
  "name": "dagre-d3",
  "version": "0.5.0",
  "description": "A D3-based renderer for Dagre",
  "author": "Chris Pettitt <chris@samsarin.com>",
  "contributors": [
    "Christian Voigt (https://github.com/christianvoigt)"
  ],
  "keywords": [
    "graph",
    "dagre",
    "graphlib",
    "renderer"
  ],
  "main": "./lib/index.js",
  "dependencies": {
    "d3": "^3.5.17",
    "ciena-dagre": "^1.0.7",
    "ciena-graphlib": "^1.0.8",
    "lodash": "^4.17.4"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run clean && npm run build-version && npm run build-lib && npm run build-dist",
    "build-dist": "webpack && uglifyjs ./dist/dagre-d3.js -c -m -o ./dist/dagre-d3.min.js --source-map ./dist/dagre-d3.min.js.map",
    "build-lib": "npm run copy-demos && babel src -d lib --presets es2015 --plugins add-module-exports",
    "build-version": "node ./src/generateVersionJs.js",
    "watch:js": "watch 'npm run build' ./src/",
    "copy-demos": "cpx \"demo/**/*{.html,.css,.js}\" dist/demo --verbose && replace-in-file ../dist/dagre-d3.js ../dagre-d3.js dist/demo/**/*",
    "clean": "rimraf lib/* && rimraf dist/*",
    "lint": "npm run lint-js",
    "lint-js": "eslint lib test *.js",
    "test": "karma start --singleRun",
    "watch:test": "karma start --browsers PhantomJS",
    "test:demo": "phantomjs ./test/demo-test.js",
    "utest": "npm run build && istanbul cover _mocha -- --recursive tests",
    "watch": "parallelshell 'npm run watch:test -s' 'npm run watch:js -s'",
    "browsersync": "browser-sync start --server 'dist/' --directory --files 'dist/**/*'",
    "dev": "parallelshell 'npm run browsersync' 'npm run watch -s'"
  },
  "devDependencies": {
    "babel-cli": "^7.0.0-alpha.12",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.7.1",
    "chai": "^1.10.0",
    "cpx": "^1.5.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coverage": "^0.4.2",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-safari-launcher": "^0.1.1",
    "mocha": "^1.21.5",
    "parallelshell": "^2.0.0",
    "phantomjs": "^2.1.3",
    "replace-in-file": "^2.5.0",
    "watch": "^1.0.2",
    "webpack": "^2.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/christianvoigt/dagre-d3.git"
  },
  "license": "MIT"
}
